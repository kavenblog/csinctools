<content name="main">
	<viewdata model="IEnumerable[[User]]"/>

  <h2>List</h2>

	<table border="0" cellpadding="0" cellspacing="0">
		<tr>
			<th width="50">Id</th>
			<th width="150">Name</th>
			<th width="150">Email</th>
		</tr>
		<tr each="var user in Model">
			<td>$!{user.Id}</td>
			<td>$!{user.Name}</td>
			<td>$!{user.Email}</td>
		</tr>
	</table>

</content>
